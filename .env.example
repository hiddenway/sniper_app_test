# RPC endpoint for Solana cluster (defaults to mainnet-beta)
SOLANA_RPC=https://api.mainnet-beta.solana.com

# HTTP port the API server listens on
PORT=5600

# SQLite database file path
DB_PATH=./data/database.sqlite

# Base58 private key of the signer; required
SIGNER_PK=replace_with_base58_private_key

# Token mint used in simple mode flows; required
INPUT_MINT_FOR_SIMPLE_MODE=replace_with_mint_address
